{
  "name": "@escapace/accept-language-parser",
  "description": "",
  "version": "1.4.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/accept-language-parser/issues",
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@ls-lint/ls-lint": "1.11.2",
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "c8": "7.11.3",
    "chai": "4.3.6",
    "codecov": "3.8.2",
    "esbuild": "0.14.46",
    "eslint": "8.18.0",
    "eslint-config-escapace": "3.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-editorconfig": "4.0.2",
    "eslint-plugin-no-null": "1.0.2",
    "fast-glob": "3.2.11",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.2",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "prettier-config-escapace": "1.0.4",
    "semantic-release": "19.0.3",
    "syncpack": "8.0.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.cjs"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/escapace/accept-language-parser",
  "license": "MPL-2.0",
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.mjs",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/escapace/accept-language-parser.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/cjs lib/esm lib/types && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --sourcemap --format=cjs --platform=node --target=node14.19.1 --outfile=./lib/cjs/index.cjs",
    "build:esm": "esbuild src/index.ts --bundle --sourcemap --format=esm --outfile=./lib/esm/index.mjs",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "prepare": "is-ci || husky install",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.mjs' && rm -rf lib/tests",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "lib/types/index.d.ts"
}
