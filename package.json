{
  "name": "@escapace/accept-language-parser",
  "description": "",
  "version": "1.4.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/accept-language-parser/issues",
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@ls-lint/ls-lint": "1.10.0",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "c8": "7.11.0",
    "chai": "4.3.6",
    "codecov": "3.8.2",
    "esbuild": "0.14.23",
    "eslint": "8.9.0",
    "eslint-config-escapace": "3.12.4",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-editorconfig": "3.2.0",
    "eslint-plugin-no-null": "1.0.2",
    "fast-glob": "3.2.11",
    "fs-extra": "10.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "lint-staged": "12.3.4",
    "mocha": "9.2.1",
    "prettier": "2.5.1",
    "prettier-config-escapace": "1.0.4",
    "semantic-release": "19.0.2",
    "syncpack": "6.2.0",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.cjs"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/escapace/accept-language-parser",
  "license": "MPL-2.0",
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.mjs",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/escapace/accept-language-parser.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/cjs lib/esm lib/types && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --sourcemap --format=cjs --platform=node --target=node16.14.0 --outfile=./lib/cjs/index.cjs",
    "build:esm": "esbuild src/index.ts --bundle --sourcemap --format=esm --outfile=./lib/esm/index.mjs",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky install",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.mjs' && rm -rf lib/tests",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "lib/types/index.d.ts"
}
